<script type="text/javascript" src="https://www.prepostseo.com/assets/lib/js/lazy.js"></script>
<script type="application/javascript">
	$("#header_search #inputSN").on("keyup" , function(){
		search_bar("#header_search");
		
	});
	$("#searchInput-banner").on("keyup" , function(){
		search_bar("#search-banner-sec");
		
	});
	
	function search_bar(id){
		if(id == "#search-banner-sec"){
			
			var search = $(""+id+" #searchInput-banner").val();
			
		}else{
			var search = $(""+id+" #inputSN").val();	
		}
		if(search.length < 2){
			return false;
		}
		if(search == ""){
			$(""+id+" .search-results").show();
			$(""+id+" .search-results .noresult").html("<span class='noresult'>No result found related to your keyword...</span>");
			return false;
		}


		if(search == ""){
			$(""+id+" .search-results").show();
			$(""+id+" .search-results .noresult").html("<span class='noresult'>No result found related to your keyword...</span>");
			return false;
		}
		var langSearch = "";
		console.log(search);

		$.ajax({
			url : "https://www.prepostseo.com/frontend/frontsearchtool",
			type:"post",
			data:{search:search,lang:langSearch},
			dataType: "json",
			success: function(resp){
				var html='';

				if(resp.fail){

					html = resp.fail;

					$(""+id+" .search-results").show();
					$(""+id+" .search-results .withoutlangSearch").html(html);
					$(""+id+" .search-results .withlangSearch").html(html);

				}else{
					$(""+id+" .search-results .withoutlangSearch").html("");
					$(""+id+" .search-results .withlangSearch").html("");
					$(""+id+" .search-results").hide();

					if(search.length < 2)
					{
						return false;
					}

					$(""+id+" .search-results").show();

					var ToolsArr= resp;
					
					if(id == "#search-banner-sec"){
			
						var val = clearText($(""+id+" #searchInput-banner").val());
						$(""+id+" .search-results .withoutlangSearch").html("");
						$(""+id+" .search-results .withlangSearch").html("");
						$(""+id+" .search-results .noresult").html("");
						$(""+id+" .search-results").hide();

					}else{
						var val = clearText($(""+id+" #inputSN").val());
						$(""+id+" .search-results .withoutlangSearch").html("");
						$(""+id+" .search-results .withlangSearch").html("");
						$(""+id+" .search-results .noresult").html("");
						$(""+id+" .search-results").hide();
					}
					
					
					
					if(val.length < 2){return false;}$(""+id+" .search-results").show();var matches = 0;
					var withoutLang = '';
					var withLang = '';
					$.each(ToolsArr, function(index, tool){

						//console.log(tool);
						var lang_tool = tool[3];
						var myVal = clearText(tool[0]);

						if(myVal != ""){
							matches++;
							if(tool[2] == "1"){
								var href = base_url+tool[1];
							}else if(tool[2] == "2"){
								var href = base_url +"calc/"+tool[1];
							}else if(tool[2] == "3"){
								var href = base_url +"tool/"+tool[1];
							}
							
							var re = new RegExp(val,"gi");
							var toolName = tool[0].replace(re, "<strong>"+val+"</strong>");
							if(lang_tool){
								withLang += '<span class="match"><a href="'+href+'">'+toolName+ '</a></span>';
							}else{
								withoutLang += '<span class="match"><a href="'+href+'">'+toolName+ '</a></span>';
							}	
							//console.log(withLang);
							//console.log(withoutLang);
						}
						if(withLang != ''){
							$(""+id+" .search-results .withoutlangSearch").html('<div class="langnamesearch"><span>All around the web</span></div>'+withoutLang);	
							$(""+id+" .search-results .withlangSearch").html('<div class="langnamesearch"><span> only</span></div>'+withLang);
						}else{
							$(""+id+" .search-results .withoutlangSearch").html(withoutLang);
							$(""+id+" .search-results .withlangSearch").html(withLang);
						}
					});
					if(matches < 1){$(""+id+" .search-results .noresult").html("<span class='noresult'>No result found related to your keyword...</span>");}
				}
			}
		});
	}
		
</script>
